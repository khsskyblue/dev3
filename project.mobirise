        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "img": {
              "border-radius": "50%",
              "width": "60px",
              "height": "60px"
            },
            ".mbr-iconfont-1": {
              "color": "@IconsColor",
              "font-size": "1rem",
              "transition": "0.7s"
            },
            ".mbr-section-title": {
              "margin-bottom": "4rem"
            },
            ".mbr-section-subtitle": {
              "color": "#ffffff",
              "margin-bottom": "1.5rem"
            },
            ".card-img": {
              "margin-bottom": "1.3rem"
            },
            ".card-title": {
              "margin-bottom": "1.5rem"
            },
            ".row": {
              "justify-content": "center"
            },
            "@media (max-width: 992px)": {
              ".card": {
